<?xml version="1.0" encoding="UTF-8"?>
<ParameterizedAlgorithmFile xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="file:hek.xsd">
    <AlgorithmDescriptionFile>rob_spoca.xml</AlgorithmDescriptionFile>
    <CodeVersion>0.5</CodeVersion>
    <Parameter>
        <ParameterName>outputDirectory</ParameterName>
        <Type>string</Type>
        <Value>/home/rtimmons/spoca/spoca_hek/results/</Value>
        <ExcludeFromCoverageID>true</ExcludeFromCoverageID>
    </Parameter>
    <Parameter>
        <ParameterName>writeEventsFrequency</ParameterName>
        <Type>numeric</Type>
        <!-- Frequency we write events to the HEK 4h-->
        <Value>14400</Value>
        <ExcludeFromCoverageID>false</ExcludeFromCoverageID>
    </Parameter>
    <Parameter>
        <ParameterName>spocaArgsPreprocessing</ParameterName>
        <Type>string</Type>
        <Value>ALC,DivMedian,DivExpTime</Value>
        <ExcludeFromCoverageID>false</ExcludeFromCoverageID>
    </Parameter>
    <Parameter>
        <ParameterName>spocaArgsNumberclasses</ParameterName>
        <Type>string</Type>
        <Value>4</Value>
        <ExcludeFromCoverageID>false</ExcludeFromCoverageID>
    </Parameter>
    <Parameter>
        <ParameterName>spocaArgsPrecision</ParameterName>
        <Type>string</Type>
        <Value>0.000000001</Value>
        <ExcludeFromCoverageID>false</ExcludeFromCoverageID>
    </Parameter>
    <Parameter>
        <ParameterName>spocaArgsBinsize</ParameterName>
        <Type>string</Type>
        <Value>0.01,0.01</Value>
        <ExcludeFromCoverageID>false</ExcludeFromCoverageID>
    </Parameter>
    <Parameter>
        <ParameterName>trackingArgsDeltat</ParameterName>
        <Type>string</Type>
        <!-- Time in seconds an AR should stay more or less at the same place 1h--> 
        <Value>3600</Value>
        <ExcludeFromCoverageID>false</ExcludeFromCoverageID>
    </Parameter>
    <Parameter>
        <ParameterName>trackingNumberImages</ParameterName>
        <Type>numeric</Type>
        <Value>9</Value>
        <ExcludeFromCoverageID>false</ExcludeFromCoverageID>
    </Parameter>
    <Parameter>
        <ParameterName>trackingOverlap</ParameterName>
        <Type>numeric</Type>
        <Value>3</Value>
        <ExcludeFromCoverageID>false</ExcludeFromCoverageID>
    </Parameter>
    <Parameter>
         <ParameterName>cCodeLocation</ParameterName>
	 <Type>string</Type>
	 <Value>/home/rtimmons/spoca/spoca_hek/bin/</Value>
	 <ExcludeFromCoverageID>true</ExcludeFromCoverageID>
    </Parameter>
    <DataSpecificationFile>
        <Filename>EUVI_for_SPOCA.txt</Filename>
        <LiveData>false</LiveData>
        <SeriesName>EUVI</SeriesName>
    </DataSpecificationFile>

</ParameterizedAlgorithmFile>
