        <Type>string</Type>
        <Value>'results/'</Value>
        <ExcludeFromCoverageID>true</ExcludeFromCoverageID>
    </Parameter>
    <Parameter>
        <ParameterName>write_events_frequency</ParameterName>
        <Type>numeric</Type>
